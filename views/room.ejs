    <div class="main">

        <div class="left__session">

                <div class="main__video">

                        <div id="video-grid">
                
                        </div>
                </div>
                <div class="video__controls">
                        
                        
                            <div class="video__controls__buttons">
                                    <div class="button button_mute" onclick="muteUnmute()">
                                        <i class="fas fa-microphone-slash"></i>
                                           <span>mute</span>
                                    </div>
                                    <div onclick="playStopVideo()" class="button video__mute">
                                        
